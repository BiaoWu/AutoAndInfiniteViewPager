<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.autoandinfiniteviewpager.demo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.autoandinfiniteviewpager.demo.AppApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.autoandinfiniteviewpager.demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.autoandinfiniteviewpager.demo.BestViewPagerDemo"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.autoandinfiniteviewpager.demo.BestViewPagerListViewDemo"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
